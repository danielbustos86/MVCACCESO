
@{
    ViewBag.Title = "CargaNombrada";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render("~/bundles/js_Nombrada")
@if (Session["MisMenus"].ToString().Contains("CARGA_MASIVA"))
{

    <div class="row">

        <div class="col-md-12">
            <h4 style="padding-top:10px;padding-bottom:10px;padding-left:5px;background-color:#F8F8F8">Cargar Nombrada</h4>

        </div>
    </div>

    <div class="row">
        <div class="col-md-3">

            <button id="btncargarNombrada" class="btn btn-success btn-block btn-lg" type="button">Cargar</button>

        </div>

        <div class="col-md-3">

            <button id="btnBuscaNombrada" class="btn btn-primary btn-block btn-lg" type="button">Buscar</button>

        </div>

    </div>
    <div class="row">


        <div class="col-md-4">
            <div class="form-group" style="margin-bottom: 5px;">
                <label class="control-label" for="Puerto">Fecha</label>


      <input class="form-control" type="date" id="fechainicioNombrada" value="11-11-1111">
              
            </div>



        </div>
        <div class="col-md-2">
            <div class="form-group" style="margin-bottom: 5px;">
                <label class="control-label" for="Puerto">Turno</label>
                <select id="SelectTurno" class="form-control">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                </select>

            </div>
        </div>




    </div>
    <div class="table-responsive">
        <table id="TablaNombrada" class="table table-striped table-bordered table-hover table-condensed">
            <thead>
                <tr>

                    <th>FECHA</th>
                    <th>TURNO</th>
                    <th>RUT</th>
                    <th>NOMBRE</th>
                    <th>APELLIDOS</th>
                    <th>LOCACION</th>
                    <th>NAVE</th>

                </tr>
            </thead>
            <tbody id="cuerpoNomb"></tbody>
        
        </table>
    </div>
}
else
{


    <div class="alert alert-danger alert-dismissible">
        <strong>Acceso Restringido!</strong> No está autorizado para ver esta página, pongase en contacto con el administrador
    </div>

    <input type="hidden" id="txt_acceso" value="SIN_ACCESO" />
}