            <div class="col-sm-5">
                <input type="text" class="form-control" id="txt_nave" style="HEIGHT: 30PX;" disabled>
            </div>
        </div>

        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_inicio" class="col-sm-2 col-form-label">Inicio</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="txt_inicio" style="HEIGHT: 30PX;" disabled>
            </div>
            <label for="txt_fin" class="col-sm-1 col-form-label">Fin</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" id="txt_fin" style="HEIGHT: 30PX;" disabled>
            </div>
        </div>


        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_empresa" class="col-sm-2 col-form-label">Empresa</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="txt_empresa" style="HEIGHT: 30PX;" disabled>
            </div>
        </div>
    </form>
    </div>


    <div class="modal fade" role="dialog" tabindex="-1" id="ModalAprobadores">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                    <h4 class="modal-title">Listado Aprobadores</h4>
                </div>
                <div class="modal-body">
                    <table id="tabla_aprobadores" class="table table-striped table-bordered table-hover table-condensed">
                        <thead>
                            <tr>
                                <th>Nombre</th>
                                <th>Telefono </th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>


                </div>
                <div class="modal-footer">
                    <button class="btn btn-default" type="button" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" role="dialog" tabindex="-1" id="ModalPatente">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                    <h4 class="modal-title">Agregar Patente</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label class="control-label" for="text-input">Patente </label>
                        <input class="form-control" type="text" name="text-input" id="txtingresaPatentecs">
                        <label class="control-label" for="text-input">Tipo </label>
                        <select id="selecttvcs" class="form-control"></select>

                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default" type="button" data-dismiss="modal">Cerrar</button>
                    <button class="btn btn-primary" type="button" id="btnguardarcs">Guardar</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" role="dialog" tabindex="-1" id="ModalUltimo">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header" style="padding-bottom: 5px;">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                    <h4 class="modal-title">PERSONA NO REGISTRA SOLICITUDES AUTORIZADAS PARA FECHA Y HORA ACTUAL</h4>
                </div>
                <div class="modal-body">
                    <h4 class="modal-title" id="title_nombre" style="margin-bottom: 10px;">LISTADO ÚLTIMAS SOLICITUDES</h4>
                    <div class="form-group">
                        <div class="table-responsive">
                            <table id="tabla_ultimas" class="table table-striped table-bordered table-hover table-condensed">
                                <thead>
                                    <tr>
                                        <th>ID SOLICITUD</th>
                                        <th style="display:none !important">nombre</th>
                                        <th>FECHA DESDE</th>
                                        <th>FECHA HASTA</th>
                                        <th>PUERTO</th>
                                        <th>LUGAR</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-default" type="button" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
    }
    else
    {


    <div class="alert alert-danger alert-dismissible">
        <strong>Acceso Restringido!</strong> No está autorizado para ver esta página, pongase en contacto con el administrador
    </div>

    <input type="hidden" id="txt_acceso" value="SIN_ACCESO" />


    }













