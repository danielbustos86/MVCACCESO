
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Scripts.Render("~/bundles/js_ingreso")

@if (Session["MisMenus"].ToString().Contains("MOVIMIENTOS"))
{
<div class="bd-example">

    <div class="col-sm-12 center-block">
        <p>.</p>
    </div>
    <form class="col-md-7">


        <div class="form-group row" style="margin-bottom: 8px;">
            <div class="col-sm-6 center-block">
                <button id="btn_ingreso" class="btn btn-default btn-lg center-block  " style=" width: 70% ; height: 60px;" type="button">ENTRADA</button>
            </div>
            <div class="col-sm-6 center-block">
                <button id="btn_salida" class="btn btn-default btn-lg center-block " style=" width: 70% ;height: 60px;" type="button">SALIDA</button>
            </div>
        </div>

        <hr style="margin-bottom: 10px;margin-top: 10px;" />



        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_rut" class="col-sm-2 col-form-label">Rut</label>
            <div class="col-sm-10">
                <input type="text" form-control" id="txt_rut" placeholder="7777777-7" style="HEIGHT: 30PX;">
            </div>
        </div>

        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_rut" class="col-sm-2 col-form-label">Pasaporte</label>
            <div class="col-sm-10">
                <input type="text" form-control" id="txt_pasaportein" placeholder="A21999" style="HEIGHT: 30PX;">
            </div>
        </div>

        <div id="mensaje_exito" class="alert alert-success alert-dismissible fade in">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
            <strong>Registro Exitoso!</strong> El registro se a creado exitosamente
        </div>


        <div id="mensaje_error" class="alert alert-danger alert-dismissible fade in">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
            <strong>No encontrado!</strong> Tu visita no se encuentra programada
        </div>



        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_solicitud" class="col-sm-2 col-form-label">Solicitud</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="txt_solicitud" style="HEIGHT: 30PX;" disabled />
            </div>
        </div>


        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_nombre" class="col-sm-2 col-form-label">Nombre</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="txt_nombre" style="HEIGHT: 30PX;" disabled />
            </div>
        </div>

        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_estado" class="col-sm-2 col-form-label">
                Estado
            </label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="txt_estado" style="HEIGHT: 30PX;" disabled />
            </div>
        </div>


        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_autorizacion" class="col-sm-2 col-form-label">Autorizacion</label>
            <div class="col-sm-7">
                <input type="text" class="form-control" id="txt_autorizacion" style="HEIGHT: 30PX;" disabled>
            </div>
            <div class="col-sm-3">
                <button class="btn btn-primary" id="btn_aprobar" style="width: 100%;" disabled>Aprobadores</button>
            </div>
        </div>




        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="cbx_vehiculo" class="col-sm-2 col-form-label">Vehiculo</label>
            <div class="col-sm-7">
                <select id="cbx_vehiculo" class="form-control" style="HEIGHT: 30PX;"></select>
            </div>
            <div class="col-sm-3">
                <button class="btn btn-primary" id="btn_modificar" style="width: 100%;" disabled>Modificar</button>
            </div>
        </div>


        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_puerto" class="col-sm-2 col-form-label">Puerto</label>
            <div class="col-sm-10">
                <input type="email" class="form-control" id="txt_puerto" style="HEIGHT: 30PX;" disabled>
            </div>
        </div>


        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_solicitante" class="col-sm-2 col-form-label">Solicitante</label>
            <div class="col-sm-10">
                <input type="email" class="form-control" id="txt_solicitante" style="HEIGHT: 30PX;" disabled>
            </div>
        </div>

        <div class="form-group row" style="margin-bottom: 8px;">
            <label for="txt_locacion" class="col-sm-2 col-form-label">Locacion</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="txt_locacion" style="HEIGHT: 30PX;" disabled>
            </div>
